{
  "name": "electron-media-org",
  "version": "0.7.0",
  "description": "Minimal and modern react+electron+webpack boilerplate",
  "author": "Lucas Heasman",
  "homepage": "https://github.com/LucasHeasman/electron-media-org",
  "repository": {
    "type": "git",
    "url": "https://github.com/LucasHeasman/electron-media-org"
  },
  "contributors": [
    "Phillip Barbiero <https://github.com/pbarbiero>",
    "Keith Weaver <https://github.com/keithweaver>",
    "Denys Dovhan <https://github.com/denysdovhan>",
    "Willy Ovalle <https://github.com/wovalle>"
  ],
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "prod": "webpack --config webpack.build.config.js && electron --noDevServer .",
    "dev": "webpack-dev-server --hot --host 0.0.0.0 --port 4000 --config=./webpack.dev.config.js",
    "build": "webpack --config webpack.build.config.js",
    "rebuild": "electron-rebuild -f -w sqlite3",
    "package": "webpack --config webpack.build.config.js",
    "postinstall": "electron-builder install-app-deps",
    "postpackage": "electron-packager ./ --overwrite --icon=src/assets/icons/win/icon.ico --out=./builds",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=src/assets/icons/mac/icon.icns --out=builds",
    "package-win": "electron-packager . electron-media-org --overwrite --asar=true --platform=win32 --arch=ia32 --icon=src/assets/icons/win/icon.ico --out=builds --version-string.CompanyName=LH --version-string.FileDescription=LH --version-string.ProductName=\"Electron Media Org\"",    
    "package-linux": "electron-packager . electron-media-org --overwrite --asar=true --platform=linux --arch=x64 --icon=src/assets/icons/png/1024x1024.png --out=builds"
  },
  "devDependencies": {
    "electron": "^1.7.8",
    "electron-builder": "^20.38.5",
    "electron-packager": "^12.2.0",
    "electron-rebuild": "^1.8.2"
  },
  "dependencies": {
    "base64-img": "^1.0.4",
    "bootstrap": "^4.2.1",
    "jquery": "^3.3.1",
    "knex": "^0.16.3",
    "react-responsive-carousel": "^3.1.46",
    "react-router-dom": "^4.3.1",
    "react-select": "^2.3.0",
    "react-sidebar": "^3.0.2",
    "reactstrap": "^7.1.0",
    "sqlite3": "^4.0.6",
    "url-loader": "^1.1.2",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.1.2",
    "babel-preset-react": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^2.28.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "style-loader": "^0.19.1",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.4.5"
  }
}
